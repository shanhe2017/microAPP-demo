    "@vueuse/core": "^7.5.4",
    "ant-design-vue": "^3.2.0",
    "axios": "^0.20.0-0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.3",
    "events": "^3.3.0",
    "highlight.js": "^11.4.0",
    "jsencrypt": "^3.2.1",
